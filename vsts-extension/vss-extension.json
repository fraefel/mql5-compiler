{
    "manifestVersion": 1,
    "id": "stpatrick-tfs-build-tasks-mql5",
    "name": "MetaTrader 5 build tasks",
    "version": "0.1.10",
    "publisher": "philip-patrick",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Build task for compiling MetaTrader 5 (MQL5) files.",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "CompileMql5Task"
        }
    ],
    "contributions": [
        {
            "id": "stpatrick-tfs-build-tasks-mql5-compile",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "CompileMql5Task"
            }
        }
    ]
}